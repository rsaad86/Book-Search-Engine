{
  "name": "book-search-engine",
  "version": "1.0.0",
  "description": "Book Search Engine",
  "main": "server/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "heroku-postbuild": "cd client && npm run build"
  },
  "keywords": [],
  "author": "RSAAD",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "dependencies": {
    "@heroku-cli/plugin-buildpacks": "^7.54.0",
    "@heroku/buildpack-registry": "^1.0.1",
    "apollo-server-express": "^3.0.1",
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "heroku": "^7.56.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rsaad86/Book-Search-Engine.git"
  },
  "bugs": {
    "url": "https://github.com/rsaad86/Book-Search-Engine/issues"
  },
  "homepage": "https://github.com/rsaad86/Book-Search-Engine#readme"
